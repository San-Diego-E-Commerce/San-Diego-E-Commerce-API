generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id String @id
  first_name String
  last_name String
  username String @unique
  email String @unique
  password String

  @@map("users")
}

model Product {
  id String @id
  name String @unique
  category String
  description String
  price String

  @@map("products")
}
